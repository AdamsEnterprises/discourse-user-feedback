{{#if model.noContent}}
    <div class='no-content'>
        {{{model.noContentHelp}}}
    </div>
{{/if}}

{{#unless guestUserWatching}}
    {{#unless currentUserWatching }}
        {{rating-composer stream=model}}
    {{/unless}}
{{/unless}}

{{#user-stream stream=model}}
    {{#each model.content as |item|}}
        {{rating-item item=item removeBookmark="removeBookmark"}}
    {{/each}}
{{/user-stream}}
